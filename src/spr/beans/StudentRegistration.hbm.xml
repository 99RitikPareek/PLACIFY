<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.00//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="spr.beans">
<class name="StudentRegistration" table="studentRegistration">
 <id name="enrollmentId" column="enrollmentid"/>
 <property name="fullName" column="fullNname"/>
 <property name="dob" column="dob"/>
 <property name="password" column="password"/>
 <property name="contact" column="contact"/>
 <property name="email" column="email"/>
 <property name="course" column="course"/>
 <property name="yearOfAcademics" column="yearofAccademics"/>
 <property name="address" column="address"/>
 <property name="city" column="city"/>
 <property name="state" column="state"/>
 <property name="tenthPercentage" column="tenthPercentage"/>
 <property name="twelvePercentage" column="twelvePercentage"/>
 <property name="graduationPercentage" column="graduationPercentage"/>
 <property name="resume" column="resume"/>
 <property name="category" column="category"/>
 <property name="cGPA" column="cGPA"/>
</class>
</hibernate-mapping>
